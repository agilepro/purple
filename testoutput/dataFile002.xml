<?xml version="1.0" encoding="UTF-8"?><web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <servlet>
    <load-on-startup>6</load-on-startup>
    <servlet-class>com.fujitsu.nugen.NGLeafServlet</servlet-class>
    <servlet-name>leaf-viewer</servlet-name>
  </servlet>
  <servlet>
    <display-name>DataFeedServlet</display-name>
    <servlet-class>com.fujitsu.nugen.DataFeedServlet</servlet-class>
    <servlet-name>DataFeedServlet</servlet-name>
  </servlet>
  <servlet>
    <load-on-startup>6</load-on-startup>
    <servlet-class>com.fujitsu.nugen.NGServlet</servlet-class>
    <servlet-name>wiki-viewer</servlet-name>
  </servlet>
  <servlet>
    <load-on-startup>5</load-on-startup>
    <servlet-class>com.fujitsu.nugen.AttachServlet</servlet-class>
    <servlet-name>attach-viewer</servlet-name>
  </servlet>
  <servlet>
    <servlet-class>com.fujitsu.nugen.RssServlet</servlet-class>
    <servlet-name>RssServlet</servlet-name>
  </servlet>
  <servlet-mapping>
    <servlet-name>leaf-viewer</servlet-name>
    <url-pattern>/p/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>leaf-viewer</servlet-name>
    <url-pattern>/b/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>leaf-viewer</servlet-name>
    <url-pattern>/s/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>leaf-viewer</servlet-name>
    <url-pattern>/u/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>wiki-viewer</servlet-name>
    <url-pattern>*.sp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>attach-viewer</servlet-name>
    <url-pattern>/attachments/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DataFeedServlet</servlet-name>
    <url-pattern>/servlet/DataFeedServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RssServlet</servlet-name>
    <url-pattern>/Tasks.rss</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>main.jsp</welcome-file>
  </welcome-file-list>
</web-app>